        <div class="results">
            <a href="/"><img class="logo" src="/img/logo.png"></a>

            <form id="query" method="get">
                <input type="text" name="q" value="{{query}}">
            </form>
        </div>
        
        <div class="entries">
        {{#each results}}    
            <div class="entry">
                <h2>{{getTitle}}</h2>
                <p>
                {{#each getAuthors}}
                    {{getForename}}, 
                {{/each}}
                </p>
            </div>
        {{/each}}
        </div>
